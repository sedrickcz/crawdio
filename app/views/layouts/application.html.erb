<!DOCTYPE html>
<%= render "/refinery/html_tag" %>
  <% site_bar = render('/refinery/site_bar', :head => true) -%>
  <head>
    <%= render "/refinery/head" %>
  </head>
  <body id="<%= canonical_id @page %>">
    <%= site_bar -%>
    <div>
      <% flash.each do |name, msg| -%>
        <%= content_tag :div, raw(msg), class: name %>
      <% end -%>
    </div>
    <div id="page_container">
      
        <%= render "/refinery/header" -%>
    

      <div id="primary" class="content-area">
      <div id="content" class="site-content" role="main">
      
        <article id="post-shop" > 
          <div class="decorator"></div>
          <div class="navbar-top-hr">
            <div class="decorator_shadow"></div>
          </div>
          <div class="border_shadow_left"></div>
          <div class="border_shadow_right"></div>
          <div class="inside_paper full_width_">
              
            <%= yield %>
          
          </div>
        </article>
      
    </div>
  </div>
</div>
    
       
     
      <footer>
        <%= render "/refinery/footer" -%>
      </footer>
    </div></div>
    <%= render "/refinery/javascripts" %>
  </body>
</html>
